{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "strict": 0,
      "no-console": 0,
      "import/extensions": 0
    }
}